<h1>Build a list of former jobs you've held</h1>
<body>
  <h1>Your Resume</h1>
  <h3>Add an old job:</h3>
  <form action='/' method='post'>
    <label for='new-job'>Job Title</label>
    <input id='new-job' name='new-job' type='text'>
    <label for='new-description'>Job Description</label>
    <input id='new-description' name='new-description' type='text'>
    <br>
    <label for='new-image'>Company address</label>
    <input id='new-image' name='new-image' type='text'>
    <button type='submit'>Add job</button>
  </form>

</body>
<h2>Your Work History</h2>
<ul>
  @foreach (var place in Model)
  {
    <li><a href="/jobs/@place.GetId()">@place.GetCity()</a></li>
  }
</ul>
